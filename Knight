static int Knight(int X, int Y){
//Ener your code here
int count=0;
if(X>2){
    //down
    if(Y>1){
        count++;
    }
    if(Y<8){
        count++;
    }
}
if(X<7){
    //right
     if(Y>1){
        count++;
    }
    if(Y<8){
        count++;
    }

}
if(Y<7){
    //up
    if(X>1){
        count++;
    }
    if(X<8){
        count++;
    }
}
if(Y>2){
     if(X>1){
        count++;
    }
    if(X<8){
        count++;
    }
}
return count;
}
